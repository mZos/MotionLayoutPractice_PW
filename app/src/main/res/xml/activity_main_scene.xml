<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="none"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1960"
        motion:staggered=".5">
        <KeyFrameSet>
            <KeyAttribute
                android:rotation="-45"
                motion:framePosition="30"
                motion:motionTarget="@+id/view1"/>
            <KeyAttribute
                android:rotation="-90"
                motion:framePosition="80"
                motion:motionTarget="@+id/view1"/>
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@+id/view1"
                motion:percentY="-0.25" />
            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@+id/view1"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="60"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@+id/view1"
                motion:percentY="-0.25" />
            <KeyPosition
                motion:framePosition="80"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@+id/view1"
                motion:percentY="0.25" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/view1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp"
            motion:layout_constraintBottom_toTopOf="@+id/view2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/textView"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@color/design_default_color_primary" />
        </Constraint>
        <Constraint
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:alpha=".5"
            android:scaleX=".5"
            android:scaleY=".5"
            motion:layout_constraintBottom_toTopOf="@+id/view3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/view1" />
        <Constraint
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:alpha=".5"
            android:scaleX=".5"
            android:scaleY=".5"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/view2" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120dp"
            android:layout_marginBottom="24dp"
            android:alpha="0"
            android:translationY="10dp"
            motion:layout_constraintBottom_toTopOf="@id/view1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/view1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toTopOf="@+id/view2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/textView"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@color/black" />
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="32dp"
            motion:layout_constraintBottom_toTopOf="@id/view1"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:id="@+id/textView" />
    </ConstraintSet>
    <Transition
        motion:autoTransition="animateToStart"
        motion:constraintSetEnd="@+id/start"
        motion:constraintSetStart="@+id/end" />
</MotionScene>